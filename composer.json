{
    "name": "spinzar/setting",
    "description": "Persistent settings package for Laravel.",
    "keywords": [
        "laravel",
        "persistent",
        "settings",
        "config"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nassim Nasibullah",
            "email": "info@spinzar.co",
            "homepage": "http://www.spinzar.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6.0|^7.0|^7.1|^7.2|^7.3|^7.4",
        "laravel/framework": "5.4.*|5.5.*|^5.6|^5.7|^5.8|^6.*|^7.*|^8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.7|^6.5|^7.5|^8.5|^9.3",
        "mockery/mockery": "0.9.*|^1.0|^1.1|^1.2|^1.3|^1.4",
        "laravel/framework": "5.4.*|5.5.*|^5.6|^5.7|^5.8|^6.*|^7.*|^8.*"
    },
    "autoload": {
    "psr-4": {
            "Spinzar\\Setting\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinzar\\Setting\\Provider"
            ],
            "aliases": {
                "Setting": "Spinzar\\Setting\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
